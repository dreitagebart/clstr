{
  "name": "@clstr/schema",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf ./dist ./node_modules",
    "push": "dotenv -e .env.local -- prisma db push",
    "generate:prisma": "prisma generate",
    "generate:schema": "tsx ./src/generate.ts",
    "format": "prisma format"
  },
  "dependencies": {
    "@clstr/auth": "workspace:*",
    "@pothos/core": "^3.41.0",
    "@pothos/plugin-prisma": "^3.64.0",
    "@prisma/client": "5.9.1",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "graphql-yoga": "^5.1.1"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "prisma": "^5.9.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}