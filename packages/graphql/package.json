{
  "name": "@clstr/graphql",
  "version": "0.1.0",
  "type": "module",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "ssr": [
        "./dist/ssr.d.ts"
      ],
      "rsc": [
        "./dist/rsc.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./ssr": "./dist/ssr.js",
    "./rsc": "./dist/rsc.js"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf ./dist ./node_modules"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.3.4",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.3",
    "@parcel/watcher": "^2.4.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "@apollo/experimental-nextjs-app-support": "^0.8.0",
    "@clstr/schema": "workspace:*",
    "@graphql-typed-document-node/core": "^3.2.0",
    "graphql": "^16.8.1"
  }
}